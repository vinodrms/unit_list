{
  "name": "UnitPal",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "async": "^1.5.2",
    "bcrypt": "^0.8.5",
    "cloudinary": "^1.3.0",
    "ejs": "~0.8.4",
    "emailjs": "^1.0.2",
    "extend": "^3.0.0",
    "grunt": "0.4.2",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.10.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "~0.9.0",
    "grunt-contrib-jst": "~0.6.0",
    "grunt-contrib-less": "0.11.1",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-watch": "^0.6.0",
    "grunt-sails-linker": "~0.9.5",
    "grunt-sync": "~0.0.4",
    "include-all": "~0.1.3",
    "moment": "^2.11.1",
    "moment-timezone": "^0.5.0",
    "mongodb": "^2.1.7",
    "node-uuid": "^1.4.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "rc": "~0.5.0",
    "sails": "~0.11.4",
    "sails-disk": "~0.10.0",
    "sails-mongo": "^0.11.6",
    "sails-test-helper": "^0.5.0",
    "sendgrid": "^2.0.0",
    "should": "^8.2.0",
    "soap": "^0.12.0",
    "swig-email-templates": "^2.0.0",
    "typescript": "^1.8.0-dev.20160120",
    "underscore": "^1.8.3",
    "util": "^0.10.3",
    "winston": "^2.1.1",
    "winston-mongodb": "^1.3.0"
  },
  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "install-deps": "npm install && gulp install-client-deps",
    "build-server": "gulp clean-server && tsc --project api",
    "build-client": "gulp clean-client && (tsc --project assets/client || echo '') && (cd assets && npm run build-client && cd ..)",
    "build-all": "npm run build-server && npm run build-client",
    "server-tests": "npm run build-server && gulp run-server-tests",
    "e2e-tests": "npm run server-tests && cd assets && npm run e2e && cd ..",
    "dev": "sails lift",
    "test": "npm run build-client && gulp run-test",
    "prod": "npm run build-client && gulp run-prod",
    "forever-stop": "forever stop UnitPalProduction || echo 'No process named UnitPalProduction found. Continuing with the rest of the build.'",
    "forever-start": "NODE_ENV=production forever -a -l /dev/null --uid \"UnitPalProduction\" start app.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ionutp/UnitPal.git"
  },
  "author": "ionutp",
  "license": "",
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-env": "^0.4.0",
    "gulp-install": "^0.6.0",
    "gulp-mocha": "^2.2.0",
    "gulp-shell": "^0.5.2"
  }
}
